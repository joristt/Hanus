x :: Int;
n :: Integer;
xa :: Integer;
xb :: Integer;

procedure fib()
{
    if n==0 then
        xa += 1;
        xb += 1;
    else
        n -= 1;
        call fib;
        xa += xb;
        swap xa xb;
    fi xa==xb;
}

procedure fibsmall()
{
    n += 2;
    call fib;
}

procedure fibmedium()
{
    n += 10;
    call fib;
}

procedure fiblarge()
{
    n += 100;
    from x == 0 do
      call fib;
      uncall fib;
      x += 1;
    until x == 1000;
}
